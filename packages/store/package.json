{
  "name": "@plumbiu/react-store",
  "version": "0.4.0",
  "description": "Less than 1kb global state management implement",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "state",
    "react",
    "store"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.mts"
      },
      "require": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    },
    "./plugins": {
      "import": {
        "types": "./dist/plugins.d.mts",
        "default": "./dist/plugins.mjs"
      },
      "require": {
        "types": "./dist/plugins.d.ts",
        "default": "./dist/plugins.js"
      }
    },
    "./shallow": {
      "import": {
        "types": "./dist/shallow.d.mts",
        "default": "./dist/shallow.mjs"
      },
      "require": {
        "types": "./dist/shallow.d.ts",
        "default": "./dist/shallow.js"
      }
    }
  },
  "author": "Plumbiu",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18"
  }
}
